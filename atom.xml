---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>PDXGit</title>
 <link href="http://grumpygits.github.com/atom.xml" rel="self"/>
 <link href="http://grumpygits.github.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://grumpygits.github.com/</id>
 <author>
   <name>Jonathan "Duke" Leto</name>
   <email>jonathan@leto.net</email>
 </author>

 {% for post in site.posts %}
    {% if post.status != 'unpublished' %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://grumpygits.github.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://grumpygits.github.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
    {% endif %}
 {% endfor %}
 
</feed>
